{
  "name": "discord.js-bot-template",
  "description": "A discord bot template using discord.js",
  "version": "0.0.1",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:dev": "nodemon run node --trace-warnings .",
    "start": "pm2 start src/index.js --name=discord.js-bot-template",
    "stop": "pm2 stop discord.js-bot-template",
    "remove": "pm2 stop discord.js-bot-template && pm2 delete discord.js-bot-template && pm2 reset discord.js-bot-template",
    "logs": "pm2 logs --lines 300 discord.js-bot-template",
    "logsError": "pm2 logs --err --lines 300 discord.js-bot-template",
    "lint": "eslint src",
    "linter": "eslint src --fix",
    "writeLinter": "eslint src --output-file linter-output.txt"
  },
  "dependencies": {
    "@discordjs/rest": "^0.5.0-dev.1652443443-d522320",
    "@mirasaki/logger": "^1.0.4",
    "common-tags": "^1.8.2",
    "discord.js": "^14.0.0-dev.1651493371-4ba0f56",
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "eslint": "^8.15.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "nodemon": "^2.0.16"
  },
  "engines": {
    "node": ">=16.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mirasaki/discord.js-bot-template.git"
  },
  "keywords": [
    "bot-template",
    "template",
    "discord",
    "discord-bot",
    "discord-bot-template"
  ],
  "author": "Richard Hillebrand (Mirasaki)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mirasaki/discord.js-bot-template/issues"
  },
  "homepage": "https://github.com/Mirasaki/discord.js-bot-template#readme"
}
